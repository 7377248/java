//接口
interface Print
{
	public void open();
	public void print(String s);
	public void close();
} 


class Canon implements Print 
{
	public void open()
	{
		System.out.println("Canon --> open");
	}
	public void print(String s)
	{
		System.out.println("Canon --> print"+ s);
	}
	public void close()
	{
		this.clean();
		System.out.println("Canon --> close");
	}
	private  void  clean()
	{
		System.out.println("Canon --> clean");
	}
}




class HP implements Print 
{
	public void open()
	{
		System.out.println("HP--> open");
	}
	public void print(String s)
	{
		System.out.println("HP--> print"+s);
	}
	public void close()
	{
		System.out.println("HP--> close");
	}
}


public class PrintFactory 
{
	public static  Print getPrint(int index)
	{
		if(index == 0)
		{
			return new HP();
		}
		else if(index == 1)
		{
			return new Canon();
		}
		else
		{
			return null;
		}
	}
}


class PrintTest 
{
	public static void main(String[] args) 
	{
		Print print = PrintFactory.getPrint(1);
		print.open();
		print.print("测试");
		print.close();
	}
}

